<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE rdf:RDF [
    <!ENTITY owl 'http://www.w3.org/2002/07/owl#'>
    <!ENTITY rdf 'http://www.w3.org/1999/02/22-rdf-syntax-ns#'>
    <!ENTITY soma 'http://www.ease-crc.org/ont/SOMA.owl#'>
    <!ENTITY xsd 'http://www.w3.org/2001/XMLSchema#'>
]>

<rdf:RDF
    xmlns:owl="&owl;"
    xmlns:rdf="&rdf;"
    xmlns:soma="&soma;"
    xmlns:xsd="&xsd;"
>
<soma:Object rdf:about="object1">
  <soma:material rdf:datatype="&xsd;string">cardboard</soma:material>
  <soma:texture rdf:datatype="&xsd;string">smooth</soma:texture>
  <soma:type rdf:datatype="&xsd;string">box of Cheez-It crackers</soma:type>
</soma:Object>

<soma:Location rdf:about="location1">
  <soma:location rdf:datatype="&xsd;string">on the table</soma:location>
  <soma:position rdf:datatype="&xsd;string">on</soma:position>
</soma:Location>

<owl:Ontology>
  <owl:imports rdf:resource="http://www.ease-crc.org/ont/SOMA.owl"/>
</owl:Ontology>

<soma:Action rdf:about="action1">
  <soma:actionType rdf:datatype="&xsd;string">grab-object</soma:actionType>
  <soma:hasObject rdf:resource="object1"/>
  <soma:hasSource rdf:resource="location1"/>
</soma:Action>

</rdf:RDF>
