</owl:Class>

<owl:Class rdf:about="&soma;ExcludedObject"
    rdfs:label="Excluded object">
  <rdfs:comment>An object that is not contained in something. This is meant very general and includes, e.g., elements excluded from a set, or things that are spatially excluded from the boundaries of some object.</rdfs:comment>
  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-OBJ.owl"/>
  <rdfs:subClassOf rdf:resource="&soma;Patient"/>
</owl:Class>

<owl:Class rdf:about="&soma;Shifting">
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:allValuesFrom>
        <owl:Restriction>
          <owl:allValuesFrom rdf:resource="&soma;Localization"/>
          <owl:onProperty rdf:resource="&dul;classifies"/>
        </owl:Restriction>
      </owl:allValuesFrom>
