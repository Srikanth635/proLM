              <owl:allValuesFrom>
                <owl:Restriction>
                  <owl:onProperty rdf:resource="&soma;hasDisposition"/>
                  <owl:someValuesFrom rdf:resource="&soma;Containment"/>
                </owl:Restriction>
              </owl:allValuesFrom>
              <owl:onProperty rdf:resource="&dul;classifies"/>
            </owl:Restriction>
          </owl:intersectionOf>
        </owl:Class>
      </owl:someValuesFrom>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf rdf:resource="&soma;FunctionalControl"/>
  <rdfs:comment>Classifies States in which an object is kept inside another object.</rdfs:comment>
  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-STATE.owl"/>
  <rdfs:label xml:lang="en">Containment state</rdfs:label>
</owl:Class>

