  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-PROC.owl"/>
  <rdfs:subClassOf rdf:resource="&soma;ForceInteraction"/>
</owl:Class>

<owl:ObjectProperty rdf:about="&soma;affordsTask"
    rdfs:label="affords task">
