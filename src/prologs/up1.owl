</owl:Class>

<owl:Class rdf:about="&soma;Predilection">
  <rdfs:label>Predilection</rdfs:label>
  <rdfs:label xml:lang="en">The relation between a 'Preference' and the 'Order' that the 'Preference' defines over Situations.

For the complete model, see 'Preference'.</rdfs:label>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:allValuesFrom>
        <owl:Class>
          <owl:unionOf rdf:parseType="Collection">
            <rdf:Description rdf:about="&soma;Preference"/>
            <owl:Class>
              <owl:intersectionOf rdf:parseType="Collection">
                <rdf:Description rdf:about="&soma;Order"/>
                <owl:Restriction>
                  <owl:allValuesFrom>
                    <owl:Restriction>
                      <owl:allValuesFrom rdf:resource="&dul;Situation"/>
                      <owl:onProperty rdf:resource="&soma;encapsulates"/>
                    </owl:Restriction>
                  </owl:allValuesFrom>
                  <owl:onProperty rdf:resource="&soma;orders"/>
                </owl:Restriction>
              </owl:intersectionOf>
            </owl:Class>
          </owl:unionOf>
        </owl:Class>
      </owl:allValuesFrom>
      <owl:onProperty rdf:resource="&dul;describes"/>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf rdf:resource="&dul;SocialRelation"/>
  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-AGENT.owl"/>
</owl:Class>

<owl:Class rdf:about="&soma;ArtificialAgent"
    rdfs:label="Artificial agent">
  <rdfs:comment>A physical object with artificial characteristics, which can perform actions to achieve desired goals, and typically has sensors and/or actuators.

There can be non-physical artificial agents such as software programs but they are not considered here in the scope of artificial agent.</rdfs:comment>
  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-OBJ.owl"/>
  <rdfs:subClassOf rdf:resource="&dul;PhysicalAgent"/>
</owl:Class>

<owl:Class rdf:about="&soma;PreservativeInteraction"
    rdfs:label="Preservative interaction">
