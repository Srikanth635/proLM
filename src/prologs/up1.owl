              <owl:onProperty rdf:resource="&dul;definesTask"/>
              <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
          </owl:intersectionOf>
        </owl:Class>
      </owl:onClass>
      <owl:onProperty rdf:resource="&dul;isDescribedBy"/>
      <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf rdf:resource="&soma;Disposition"/>
</owl:Class>

